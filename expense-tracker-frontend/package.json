{
  "name": "expense-tracker",
  "version": "0.0.5-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "generate-api": "openapi-generator-cli generate -g typescript-angular -i ../openApi/schema.yaml -o ./build/expense-tracker-frontend-api",
    "startMockServer": "prism mock -d ../openApi/schema.yaml",
    "prestart": "npm run generate-api",
    "start": "ng serve -o --verbose --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check true --ssl true",
    "startWithMock": "ng serve --configuration mocked-development --host 0.0.0.0 --disable-host-check true",
    "startWithLocal": "ng serve --configuration development --host 0.0.0.0 --disable-host-check true --ssl true",
    "prebuild": "npm run generate-api",
    "build": "ng build",
    "build-watch": "ng build --watch --configuration development",
    "pretest": "npm run generate-api",
    "test": "ng test --watch=false --code-coverage --browsers chromeHeadless",
    "test-watch": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/material": "^14.1.0",
    "@angular/material-moment-adapter": "^14.2.7",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@handsontable/angular": "^12.1.2",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "feather-icons": "^4.29.0",
    "jquery": "^3.6.0",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0",
    "uuid": "^9.0.0",
    "zone.js": "^0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@types/feather-icons": "^4.7.0",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^18.6.0",
    "jasmine-core": "^4.3.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
